<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>CFBundleURLTypes</key>
    <array>
      <dict>
        <key>CFBundleURLName</key>
        <!-- Obviously needs to be replaced with your app's bundle identifier -->
        <string>io.oeoe.tauri-nextjs-boilerplate</string>
        <key>CFBundleURLSchemes</key>
        <array>
          <!-- register the myapp:// and myscheme:// schemes -->
          <string>tauri-nextjs-boilerplate-app-scheme</string>
          <string>tauri-nextjs-boilerplate-app-scheme-second</string>
        </array>
      </dict>
    </array>
  </dict>
</plist>